{
    "name": "pnhs/parser_nf",
    "description": "Biblioteca faz a conversão do xml de nf-e e nfc-e brasileira para um objeto PHP, com a intenção de facilitar a importação para o sistema.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Pnhs\\ParserNF\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Nicola Serafim",
            "email": "nicola@seraf.im"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=8.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.15"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
